{
	"name": "Edge-AI",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Microsoft Edge Browser extension to ask questions about the current page.",
	"icons": {
		"128": "icons/edge-ai-128.png"
	},
	"permissions": [
		"contextMenus"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			]
		}
	]
}